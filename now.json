{
    "name": "graphqlmocks",
    "version": 2,
    "builds": [
        {
            "src": "src/*.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "methods": [
                "GET",
                "POST"
            ],
            "dest": "/src/index.js"
        }
    ]
}